<Window x:Name="HelpDesk_Tool" x:Class="HelpDesk_Tool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelpDesk_Tool"
        mc:Ignorable="d"
        Title="HelpDesk Tool" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Locked Users" HorizontalAlignment="Left" Margin="19,19,0,0" VerticalAlignment="Top" Width="75"/>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="100" Margin="10,79,0,0" VerticalAlignment="Top" Width="497">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="User Name"/>
                    <GridViewColumn Header="Locked Time"/>
                    <GridViewColumn Header="PasswordLastSet"/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
